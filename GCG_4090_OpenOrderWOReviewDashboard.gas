Program.Sub.cmdjumpto_click.Start
F.Intrinsic.Control.SetErrorHandler("cmdjumpto_click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.iRecords.Declare(Long)
V.Local.sLabel.Declare(String)

F.Intrinsic.Control.If(V.Screen.F_Main!txtPage.Text,<=,V.Global.iPages)
	'Update current page
	V.Global.iCurrentPage.Set(V.Screen.F_Main!txtPage.Text)
	'Calculate last record
	F.Intrinsic.Math.Mult(V.Global.iCurrentPage,500,V.Local.iRecords)
	'Calculate the beginning record number
	F.Intrinsic.Math.Sub(V.Local.iRecords,500,V.Global.iBegRec)
	'Check last record to make sure we aren't over the ubound of the UDT
	F.Intrinsic.Control.If(V.Local.iRecords,>,V.uGlobal.uGrid2.UBound)
		V.Global.iLastRec.Set(V.uGlobal.uGrid2.UBound)
	F.Intrinsic.Control.Else
		'subtract 1 to get a total of 500 records
		F.Intrinsic.Math.Sub(V.Local.iRecords,1,V.Global.iLastRec)
	F.Intrinsic.Control.EndIf

	'Update Label
	F.Intrinsic.String.Build("Page {0} of {1}",V.Screen.F_Main!txtPage.Text,V.Global.iPages,V.Local.sLabel)
	Gui.F_Main.lblPageNumber.Caption(V.Local.sLabel)

	Gui.F_Main.fgSales.Visible(False)
	F.Intrinsic.Control.CallSub(Newloadgrid)
	Gui.F_Main.fgSales.Visible(True)
F.Intrinsic.Control.EndIf
F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdjumpto_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_4090_OpenOrderWOReviewDashboard.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Subexit)
Function.Intrinsic.Control.EndIf


Program.Sub.cmdjumpto_click.End

