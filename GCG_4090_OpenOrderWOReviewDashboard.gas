V.Local.iC1.Declare(Long)
V.Local.sSql.Declare(String)
V.Local.fPages.Declare(Float)
V.Local.iRows.Declare(Long)

'Remove any null dates from the grid
F.Intrinsic.Variable.UDTMultiSeekSet(V.uGlobal.uGrid2!ShiptoOP,"1/1/1900",V.uGlobal.uGrid2!ShiptoOP,"")
F.Intrinsic.Variable.UDTMultiSeekSet(V.uGlobal.uGrid2!ShiptoOP,"1/1/2039",V.uGlobal.uGrid2!ShiptoOP,"")
F.Intrinsic.Variable.UDTMultiSeekSet(V.uGlobal.uGrid2!Item_Promise_Dt,"1/1/1900",V.uGlobal.uGrid2!Item_Promise_Dt,"")
F.Intrinsic.Variable.UDTMultiSeekSet(V.uGlobal.uGrid2!Item_Promise_Dt,"1/1/2039",V.uGlobal.uGrid2!Item_Promise_Dt,"")

'Make sure that there is at least 1 record in the UDT, Last rec gets set to the ubound of the UDT if it doesn't meet the minimum requirements of 500 records
F.Intrinsic.Control.If(V.Global.iLastRec,<>,-1)
	F.Intrinsic.Control.If(V.Global.iPart,=,0)
		Gui.F_Main.fgSales.LoadFromUDT("V.uGlobal.uGrid2","Row::0*!*Order_NO::1*!*Record_NO::2*!*Job::3*!*Suffix::4*!*Project::5*!*FLAG_WO_PRTD::6*!*Customer::7*!*City::8*!*State::9*!*Part::10*!*Product_Line::11*!*SalesPerson::12*!*User_1::13*!*Description::14*!*Item_Promise_Dt::15*!*Qty_BO::16*!*Qty_OnHand::17*!*PcsRem::18*!*MatIssued::19*!*MaterialShortage::20*!*HOURS_ESTIMATED::21*!*NUM_HOURS::22*!*REMHRS::23*!*CurrentOP::24*!*NextOP::25*!*OutsideProcess::26*!*ShiptoOP::27*!*WO_NOTE::28",-1,V.Global.iBegRec,V.Global.iLastRec)
		Gui.F_Main.fgSales.TextMatrix(10,0,"Part Number")

	F.Intrinsic.Control.Else
		Gui.F_Main.fgSales.LoadFromUDT("V.uGlobal.uGrid2","Row::0*!*Order_NO::1*!*Record_NO::2*!*Job::3*!*Suffix::4*!*Project::5*!*FLAG_WO_PRTD::6*!*Customer::7*!*City::8*!*State::9*!*Customer_Part::10*!*Product_Line::11*!*SalesPerson::12*!*User_1::13*!*Description::14*!*Item_Promise_Dt::15*!*Qty_BO::16*!*Qty_OnHand::17*!*PcsRem::18*!*MatIssued::19*!*MaterialShortage::20*!*HOURS_ESTIMATED::21*!*NUM_HOURS::22*!*REMHRS::23*!*CurrentOP::24*!*NextOP::25*!*OutsideProcess::26*!*ShiptoOP::27*!*WO_NOTE::28",-1,V.Global.iBegRec,V.Global.iLastRec)
		Gui.F_Main.fgSales.TextMatrix(10,0,"Customer Part")
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf

F.Intrinsic.Math.Sub(V.Screen.F_Main!fgSales.Rows,2,V.Local.iRows)

F.Intrinsic.Control.For(V.Local.iC,0,V.Local.iRows,1)
	F.Intrinsic.Math.Add(V.Local.iC,1,V.Local.iC1)
	Gui.F_Main.fgSales.TextMatrix(0,V.Local.iC1,V.Local.iC1)
	F.Intrinsic.Control.If(V.uGlobal.uGrid2(v.Local.iC)!MatIssued,"=","Y")
			Gui.F_Main.fgSales.GSGOverrideProperty(19,V.Local.iC1,"BackColor",V.color.Green)
	F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.If(V.uGlobal.uGrid2(v.Local.iC)!MaterialShortage,"=","Y")
			Gui.F_Main.fgSales.GSGOverrideProperty(20,V.Local.iC1,"BackColor",V.color.Red)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Next(V.Local.iC)

V.Global.iRowCount.Set(V.Local.iC)

'this needs to be set two 2 rows: title and record
F.Intrinsic.Control.If(V.uGlobal.uGrid2.UBound,=,0)
	Gui.F_Main.fgSales.Rows(2)
F.Intrinsic.Control.EndIf

V.Local.sSql.set("select sum(HOURS_ESTIMATED) as Hour,sum(NUM_HOURS) as Num,sum(REMHRS) as Rem from GCG_4090_DATA")

F.ODBC.Connection!Conx.OpenLocalRecordsetRO("rst",V.Local.sSql)
	F.Intrinsic.Control.if(V.ODBC.Conx!rst.EOF,=,False)
		Gui.F_Main.txtTotEstHrs.Text(V.ODBC.Conx!rst.FieldVal!Hour)
		Gui.F_Main.txtActHrs.Text(V.ODBC.Conx!rst.FieldVal!Num)
		Gui.F_Main.txtRemHrs.Text(V.ODBC.Conx!rst.FieldVal!Rem)
	F.Intrinsic.Control.endif
F.ODBC.Conx!rst.Close

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("NewLoadGrid_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_4090_OpenOrderWOReviewDashboard.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
