Program.Sub.NewUpdate.Start
F.Intrinsic.Control.SetErrorHandler("NewUpdate_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)

V.Local.sSQL.Set("update GCG_4090_DATA set Qty_BO = Qty_Order where GCG_4090_DATA.Job IS  NOT NULL and (Parent_wo IS NOT NULL and ltrim(Parent_wo) <> '') ")
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA set Qty_BO = Qty_Order where GCG_4090_DATA.Job IS  NOT NULL and (Parent_wo IS NULL and ltrim(Parent_wo) = '') and GCG_4090_DATA.ORDER_NO IS NULL ")
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA set FLAG_WO_PRTD = '1' where  FLAG_WO_PRTD in ('Y','R')")
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA set HOURS_ESTIMATED =  (select sum(hours_estimated) from v_job_operations where GCG_4090_DATA.job = v_job_operations.job    ")
F.Intrinsic.String.Build("{0} and GCG_4090_DATA.suffix = v_job_operations.suffix and  LMO = 'L' group by GCG_4090_DATA.job,GCG_4090_DATA.suffix) ",V.Local.sSQL,V.Local.sSQL)
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA set RemHrs = round(HOURS_ESTIMATED - NUM_HOURS,2)  ")
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA set MatIssued =  (SELECT if(sum(APS.PcsRemain)=0,'Y','') as ss FROM APSV3_JBLINES APS WHERE APS.Seqtype = 2   ")
F.Intrinsic.String.Build("{0} and APS.Seq < 990000 and GCG_4090_DATA.job=APS.job and GCG_4090_DATA.suffix = APS.sfx  group by APS.job,APS.sfx) ",V.Local.sSQL,V.Local.sSQL)
F.ODBC.Connection!Conx.Execute(V.Local.sSQL)

V.Local.sSQL.Set("update GCG_4090_DATA   set MatIssued   = 'Y' where EXISTS (SELECT  distinct APS1.job + APS1.sfx  FROM APSV3_JBLINES APS1 WHERE GCG_4090_DATA.job =  APS1.Job and   ")
F.Intrinsic.String.Build("{0} GCG_4090_DATA.suffix = APS1.SFX and APS1.Seqtype = 2 and APS1.Seq < 990000 and APS1.compldate is not null and   APS1.job + APS1.sfx not in ",V.Local.sSQL,V.Local.sSQL)
F.Intrinsic.String.Build("{0} (SELECT APS2.job + APS2.sfx  FROM APSV3_JBLINES APS2 WHERE APS2.Seqtype = 2 and APS1.job = APS2.job  and APS1.sfx=APS2.sfx and APS2.Seq < 990000 and APS2.compldate is  null ))",V.Local.sSQL,V.Local.sSQL)
